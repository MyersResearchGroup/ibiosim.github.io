      <functionDefinition id="total" name="Find total molecules">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <bvar>
              <ci> y </ci>
            </bvar>
            <apply>
              <plus/>
              <ci> x </ci>
              <apply>
                <times/>
                <cn type="integer"> 2 </cn>
                <ci> y </ci>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
