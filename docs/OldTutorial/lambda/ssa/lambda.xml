            <bvar>
              <ci> a </ci>
            </bvar>
            <bvar>
              <ci> b </ci>
            </bvar>
            <apply>
              <divide/>
              <apply>
                <plus/>
                <ci> a </ci>
                <ci> b </ci>
              </apply>
              <cn type="integer"> 2 </cn>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
